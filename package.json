{
  "name": "reserve",
  "type": "module",
  "scripts": {
    "build:ui": "vite build",
    "dev": "pnpm run build:ui && wrangler dev",
    "deploy": "pnpm run build:ui && wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "test": "vitest run"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.6",
    "hono": "^4.11.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.10",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "vite": "^6.2.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.4.0"
  }
}
